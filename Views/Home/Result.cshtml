@model Survey
<body>
    <div class="container text-center border mt-5">
        <div class="row">
            <div class="col-md-5 p-3">
                <label>Name:</label>
            </div>
            <div class="col-md-5 p-3">
                <p>
                    @Model.Name
                </p>

            </div>
        </div>
        <div class="row">
            <div class="col-md-5 p-3">
                <label>Dojo Location:</label>
            </div>
            <div class="col-md-5 p-3">
                <p>
                    @Model.Location
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-5 p-3">
                <label>Favorite Language:</label>
            </div>
            <div class="col-md-5 p-3">
                <p>
                    @Model.Language
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-5 p-3">
                <label>Comment:</label>
            </div>
            <div class="col-md-5 p-3">
                <p>
                    @Model.Comment
                </p>
            </div>
        </div>
        <a href="/" role="button" class="btn btn-danger mb-4">Go Back!</a>
    </div>
    
</body>